# Nillion SecretLLM API keys
NILLION_API_KEY=your_nillion_api_key
NILLION_NODE_URL=https://nilai-demo.nillion.network/v1

# Ethereum configuration
ETHEREUM_PRIVATE_KEY=your_ethereum_private_key
ETHEREUM_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID
INFURA_API_KEY=your_infura_api_key
T1_CONTRACT_ADDRESS=your_contract_address

# Coinbase AgentKit
COINBASE_CDP_KEY=your_coinbase_cdp_key
COINBASE_CDP_SECRET=your_coinbase_cdp_secret
COINBASE_CDP_CLIENT_KEY=your_coinbase_cdp_client_key

ETHERSCAN_API_KEY=your_etherscan_api_key

# Privy
PRIVY_APP_ID=your_privy_app_id
PRIVY_APP_SECRET=your_privy_app_secret
PRIVY_JWKS_ENDPOINT=https://auth.privy.io/api/v1/apps/YOUR_APP_ID/jwks.json

# Azure
AZURE_API_KEY=your_azure_api_key
AZURE_SERVICE_ENPOINT=https://your-azure-service.openai.azure.com/

# Recall
RECALL_PRIVATE_KEY=your_recall_private_key
RECALL_BUCKET_ALIAS=auto-trader-memory
RECALL_BUCKET_ADDRESS=your_recall_bucket_address
RECALL_COT_LOG_PREFIX="cot"
RECALL_BATCH_SIZE=4
RECALL_SYNC_INTERVAL=120000
RECALL_NETWORK=testnet

# Azure OpenAI
AZURE_OPENAI_API_KEY=your_azure_openai_api_key
AZURE_OPENAI_ENDPOINT=https://your-instance-name.openai.azure.com
AZURE_OPENAI_API_INSTANCE_NAME=your_instance_name
AZURE_OPENAI_API_DEPLOYMENT_NAME=your_deployment_name
AZURE_OPENAI_API_VERSION=2023-12-01-preview

# App Settings
PORT=3200
NODE_ENV=development
MARLIN_ENCLAVE=false
CDP_NETWORK_ID=base-sepolia

ENV_EXPORT_PATH=/absolute/path/to/.env.export

# Chain Configuration
CHAIN_ID=84532

# LLM Provider Configuration
LLM_PROVIDER=gemini
PREFERRED_LLM_PROVIDER=gemini

# Google Generative AI (Gemini)
GOOGLE_API_KEY=your_google_api_key

# Mock Wallet
MOCK_WALLET_ADDRESS=0xYourMockWalletAddress

# Tavily
TAVILY_API_KEY=your_tavily_api_key

# WebSocket
ENABLE_WEBSOCKETS=true
WS_PORT=3201

# Autonomous Trading
ENABLE_AUTONOMOUS_MODE=true
AUTONOMOUS_INTERVAL_MINUTES=1
RISK_LEVEL=medium

# Wallet DB Configuration
WALLET_DB_DIR=data/wallets
WALLET_ENCRYPTION_KEY=your_secure_encryption_key_min_32_chars
MNEMONIC_PHRASE=your_wallet_recovery_phrase
