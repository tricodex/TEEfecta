# Test Environment Configuration

# ---- CDP API Configuration ----
# Using mock values for testing
COINBASE_CDP_KEY=mock_key
COINBASE_CDP_SECRET=mock_secret
CDP_NETWORK_ID=base-sepolia

# ---- Recall Network Configuration ----
# Use existing RECALL_PRIVATE_KEY from environment
# RECALL_PRIVATE_KEY is set in the environment
RECALL_BUCKET_ALIAS=4g3n7-test-bucket
RECALL_NETWORK=testnet

# ---- LLM Configuration ----
# Use mock LLM for testing
PREFERRED_LLM_PROVIDER=mock

# ---- AgentKit Configuration ----
# Enable both agents for testing
ENABLE_AGENTKIT=true
ENABLE_COLLABORATION=true
ENABLE_ACTION_PROVIDERS=true

# ---- Marlin CVM Configuration ----
# Not running in enclave for testing
MARLIN_ENCLAVE=false

# ---- Tavily Search ----
# Use mock search to avoid using credits
USE_MOCK_SEARCH=true

# ---- API Server Configuration ----
PORT=3001
HOST=localhost

# ---- Ethereum Configuration ----
# Mock wallet for testing
MOCK_WALLET_ADDRESS=0x8070591331daa4b7f1e783a12b52890a6917d98d

# ---- Autonomous Trading Configuration ----
# Disable autonomous mode for testing
ENABLE_AUTONOMOUS_MODE=false
