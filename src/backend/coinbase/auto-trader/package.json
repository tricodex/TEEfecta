{
  "name": "coinbase-auto-trader",
  "version": "1.0.0",
  "description": "Coinbase Auto Trader",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --target node --outdir dist",
    "clean": "rm -rf dist",
    "clean:build": "bun run clean && bun run build",
    "start": "bun run src/index.ts",
    "dev": "nodemon",
    "test": "bun test",
    "test-azure": "bun test ./tests/azure.test.ts --timeout 30000",
    "test-recall": "bun test ./tests/recall.test.ts",
    "verify-recall": "bun run src/services/verify-recall.ts",
    "test-integration": "bun test ./tests/integration.test.ts",
    "test-e2e": "bun test ./tests/e2e.test.ts",
    "test-all": "bun test ./tests/*.test.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.10",
    "@coinbase/agentkit": "latest",
    "@langchain/core": "^0.2.0",
    "@langchain/openai": "^0.0.10",
    "@privy-io/server-auth": "latest",
    "axios": "^1.8.4",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "langchain": "^0.0.200",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.1",
    "@types/uuid": "^9.0.7",
    "bun-types": "^1.2.5",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
